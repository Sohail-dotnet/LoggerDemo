{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    //shows Information and above (Info, Warning, Error)
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    },
    //shows Error and above (Error, Critical)
    "Debug": {
      "LogLevel": {
        "Default": "Error"
      }
    }
  },
  "AllowedHosts": "*"
}
